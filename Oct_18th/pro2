public class class_base 
{
protected int a=40;
protected void disp()// this are the protected members of base class
{
System.out.println("Hiii this is sneha");	
}
}

public class derived extends class_base
{

	public static void main(String[] args)
	{
		derived d1=new derived();
		d1.disp();
		System.out.println("Value of a="+d1.a); // The protected members only we can access from the derived class
  }

}

public class class_derived1 
{

	public static void main(String[] args)
	{
		class_base  b1=new class_base ();
		b1.disp();
		System.out.println("Value of a="+d1.a); // The protected members are not accessible from the outside class
  }
	}

}

